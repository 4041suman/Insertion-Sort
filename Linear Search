import java.util.*;
class LinearSearch{
    public static void main(String[] args) {
            int arr[]={5,9,17,45,59,63,71,89,33,22,1};
            int key=22;
            int flag=0;
     for(int i=0;i<arr.length;i++){
         if(arr[i]==key){
             flag=1;
             System.out.println("key found at "+i);
             break;
             
         }
     }
     if(flag==0){
         System.out.println("Key is not found");
     }
    }
}
